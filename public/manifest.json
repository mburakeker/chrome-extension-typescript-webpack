{
  "manifest_version": 3,
  "name": "My Chrome Extension",
  "description": "This extension does something",
  "version": "0.0.0",
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_icon": {
      "128": "images/icon-128.png"
    }
  }
}
